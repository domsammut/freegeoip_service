<?xml version="1.1" encoding="UTF-8"?>
<extension id="freegeoip_service" status="released" xmlns="http://symphony-cms.com/schemas/extension/1.0">
    <name>FreeGeoIP Service</name>
    <description>Returns a users geo location information based off the awesome freegeoip.net API</description>
    <repo type="github">https://www.github.com/domsammut/freegeoip_service</repo>
    <url type="discuss">http://www.getsymphony.com/discuss/thread/106134/</url>
    <types>
        <type>Third Party Integration</type>
    </types>
    <authors>
        <author>
            <name github="domsammut" symphony="domsammut">Dom Sammut</name>
            <website>https://domsammut.com/</website>
        </author>
    </authors>
    <releases>
        <release version="0.3.2" date="2015-09-26" min="2.3.1">
            - Confirm compatibility with Symphony 2.6.*
        </release>
        <release version="0.3.1" date="2014-05-19" min="2.3.1" max="2.4">
            - Confirm compatibility with Symphony 2.4
        </release>
        <release version="0.3" date="2014-04-26" min="2.3.1" max="2.3.6">
            - Removed superfluous Response XML node that was simply an extra wrapper
            - Changed root node to conform to Symphony naming practices
        </release>
        <release version="0.2" date="2014-04-25" min="2.3.1" max="2.3.6">
            - Requests now made over https.
        </release>
        <release version="0.1" date="2014-04-25" min="2.3.1" max="2.3.6">
            - Initial public release
        </release>
    </releases>
</extension>